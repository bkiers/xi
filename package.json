    "sequelize-typescript": "^1.1.0",
    "sqlite": "^4.0.19",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.6.5",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
